///fonction de recherche de primalite
#include <iostream>

using namespace std;

bool isPrime(int n)
{
    bool isPrime= true;
    
    for(int i=2;i<=n/2;i++)
    {
        if(n%i==0)
        {
            isPrime=false;    
        }
    }
    return(isPrime);
}
int main()
{
    bool result;
    result = isPrime(85);
   cout <<result<< endl;
}
